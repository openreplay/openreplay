	--   or st.admission_number like '%maya%'
	--   or u.aadhar_number like '%maya%'
	-------------------------------------------
	-- SECTIONS
	-------------------------------------------
	-- select
	--   *
	-- from
	--   "tnt_67201e5a-f3b8-48fb-ae8a-f8484ceed5a1".sections s
